{
  "name": "dino-overlay-widget",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:widget": "vite build",
    "build:cdn": "npm run build:widget && node scripts/build-cdn-snippet.js",
    "build:security": "node scripts/generate-integrity.js",
    "build:production": "npm run build:cdn && npm run build:security",
    "deploy:cdn": "node scripts/deploy-cdn.js deploy",
    "rollback:cdn": "node scripts/deploy-cdn.js rollback",
    "dev": "next dev",
    "dev:widget": "vite build --watch",
    "lint": "next lint",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=test/core",
    "test:integration": "jest --testPathPattern=test/integration",
    "test:accessibility": "jest --testPathPattern=test/accessibility",
    "test:performance": "npm run build:widget && jest --testPathPattern=test/performance",
    "test:api": "jest --testPathPattern=test/api",
    "test:api:watch": "jest --testPathPattern=test/api --watch",
    "test:api:coverage": "jest --testPathPattern=test/api --coverage",
    "test:bundle-size": "npm run build:widget && jest test/performance/bundle-size.test.ts",
    "test:load-time": "npm run build:widget && jest test/performance/load-time.test.ts",
    "test:e2e": "playwright test e2e/overlay.spec.ts e2e/user-workflows.spec.ts",
    "test:e2e:cross-browser": "playwright test e2e/cross-browser-compatibility.spec.ts",
    "test:e2e:accessibility": "playwright test e2e/accessibility.spec.ts",
    "test:e2e:all": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:comprehensive": "node test/run-comprehensive-tests.js",
    "test:ci": "npm run test:comprehensive",
    "test:deployment": "node scripts/run-deployment-tests.js",
    "test:deployment:quick": "node scripts/run-deployment-tests.js --skip-e2e --skip-performance",
    "test:integration:real-estate": "jest test/integration/real-estate-integration.test.ts",
    "test:integration:cdn": "jest test/integration/cdn-deployment.test.ts",
    "test:integration:api": "jest test/integration/api-workflow.test.ts",
    "test:deployment:validation": "jest test/integration/deployment-validation.test.ts",
    "verify:deployment": "npm run build:production && npm run test:deployment",
    "analyze:bundle": "npm run build:widget && node scripts/analyze-bundle.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
